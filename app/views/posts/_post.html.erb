<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= post.content %>
  </p>

  <p>
    <strong>Kind:</strong>
    <%= post.kind %>
  </p>

  <p>
    <strong>Category:</strong>
    <%= post.category %>
  </p>

  <p>
    <strong>Tag:</strong>
    <%= post.tag %>
  </p>

</div>
